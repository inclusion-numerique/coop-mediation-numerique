{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "extends": ["../../biome.json"],
  "overrides": [
    {
      "include": [
        "*.ts",
        "*.tsx"
      ],
      "javascript": {
        "formatter": {
          "quoteStyle": "single"
        },
